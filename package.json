{
  "name": "term3",
  "main": "./index",
  "version": "0.22.1",
  "description": "A terminal emulator for Atom. You can run shell sessions, Vim, Emacs, htop, etc.",
  "repository": "https://github.com/Floobits/atom-term3",
  "license": "MIT",
  "engines": {
    "atom": ">=0.121.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "debounce": "1.0.x",
    "event-kit": "1.5.x",
    "fs-plus": "^2.2.3",
    "ptyw.js": "0.4.x",
    "react-atom-fork": "0.11.x",
    "react-tools": "0.11.x",
    "redux": "^3.6.0",
    "xterm": "^2.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier": "^1.2.2"
  },
  "scripts": {
    "build": "npm run install",
    "lint": "eslint lib templates",
    "watch": "jsx --watch --harmony templates lib/build",
    "precommit": "lint-staged",
    "install": "jsx --harmony templates lib/build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --trailing-comma all",
      "eslint",
      "git add"
    ]
  },
  "providedServices": {
    "term3-service": {
      "description": "Provides a service object with the methods: newTerm, onTerm, and getTerminals.",
      "versions": {
        "0.1.3": "service_0_1_3"
      }
    }
  },
  "deserializers": {
    "term3/TermView": "deserializeTermView"
  }
}
